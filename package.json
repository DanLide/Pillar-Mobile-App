{
  "name": "RepairStack",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "env:qa": "cp .env.qa .env",
    "env:uat": "cp .env.uat .env",
    "env:prod": "cp .env.prod .env",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:qa": "yarn ios --scheme=RepairStackQA",
    "ios:uat": "yarn ios --scheme=RepairStackUAT",
    "ios:prod": "yarn ios --scheme=RepairStackPROD",
    "start": "react-native start",
    "archive:qa": "xcodebuild -workspace ios/RepairStack.xcworkspace -scheme \"RepairStackQA\" -archivePath build/RepairStack.xcarchive archive",
    "archive:uat": "xcodebuild -workspace ios/RepairStack.xcworkspace -scheme \"RepairStackUAT\" -archivePath build/RepairStack.xcarchive archive",
    "archive:prod": "xcodebuild -workspace ios/RepairStack.xcworkspace -scheme \"RepairStackPROD\" -archivePath build/RepairStack.xcarchive archive",
    "generate-ipa": "xcodebuild -exportArchive -archivePath build/RepairStack.xcarchive -exportPath build -exportOptionsPlist ios_export/src/ExportOptions.plist",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package && react-native setup-ios-permissions && cd ios && pod install"
  },
  "reactNativePermissionsIOS": [
    "Camera",
    "BluetoothPeripheral",
    "LocationWhenInUse"
  ],
  "dependencies": {
    "@microsoft/applicationinsights-react-native": "^4.2.0",
    "@microsoft/applicationinsights-web": "^3.2.1",
    "@react-native-async-storage/async-storage": "^1.19.1",
    "@react-native-clipboard/clipboard": "^1.12.1",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "base-64": "^1.0.0",
    "date-fns": "^2.30.0",
    "deprecated-react-native-prop-types": "^4.0.0",
    "gtin": "^1.0.2",
    "i18next": "^23.11.3",
    "jwt-decode": "^3.1.2",
    "lottie-react-native": "^6.4.1",
    "mobx": "^6.8.0",
    "mobx-react": "^7.6.0",
    "ramda": "^0.28.0",
    "react": "18.2.0",
    "react-i18next": "^14.1.1",
    "react-native": "0.71.18",
    "react-native-azure-auth": "^1.8.8",
    "react-native-blob-util": "^0.19.4",
    "react-native-bluetooth-state-manager": "^1.3.5",
    "react-native-circular-progress": "^1.3.9",
    "react-native-config": "^1.5.1",
    "react-native-confirmation-code-field": "^7.3.1",
    "react-native-device-info": "^10.7.0",
